    "build": "webpack -d --watch"
  },
  "dependencies": {
    "axios": "^0.19.0",
    "babel-jest": "^24.8.0",
    "cors": "^2.8.5",
    "express": "^4.17.3",
    "mongoose": "^5.6.3",
    "morgan": "^1.9.1",
    "react": "^16.8.6",
    "react-dom": "^16.8.6",
    "react-icons": "^3.7.0",
    "webpack": "^4.35.3"
  },
  "devDependencies": {
    "@babel/core": "^7.5.5",
    "@babel/preset-env": "^7.5.5",
    "@babel/preset-react": "^7.0.0",
    "babel-loader": "^8.0.6",
    "enzyme": "^3.10.0",
    "enzyme-adapter-react-16": "^1.14.0",
    "eslint-config-hackreactor": "git://github.com/reactorcore/eslint-config-hackreactor",
    "jest": "^24.8.0",
    "webpack-cli": "^3.3.5"
  }
